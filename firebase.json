{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/helloWorld",
        "function": "app"
      }
    ],
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
  },
  "functions": {
    "predeploy": "npm --prefix functions run build",
    "source": "functions"
  }
}
